<manifest xmlns:tools="http://schemas.android.com/tools"
          package="com.erajiezhang"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_interceptor"
        android:label="@string/string_btn_interceptor"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/icon_interceptor"
        android:supportsRtl="true"
        tools:ignore="GoogleAppIndexingWarning"
        android:theme="@style/TXThem">


        <activity
            android:name=".activity.WelcomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.Mainacitvity"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"/>
        <activity
            android:name=".activity.ShowClockActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>
        <activity android:name=".activity.ThreelevellinkageActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>
        <activity android:name=".activity.ShowImageActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>
        <activity android:name=".activity.GobangActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>
        <activity android:name=".activity.LoginActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>
        <activity android:name=".powersurvey.PowerSurveyMainActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            android:label="@string/app_name"/>

    </application>
</manifest>
